<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>


    <!-- 静态网页 -->
    <rule>
        <from>^/report/([A-Za-z0-9_-]+).html$</from>
        <to>/sys/report/page?mid=$1</to>
    </rule>
    <!-- 静态网页 -->
    <rule>
        <from>^/botton/([A-Za-z0-9_-]+)/([A-Za-z0-9_-]+).html$</from>
        <to>/sys/report/botton?mid=$1&amp;bid=$2</to>
    </rule>
</urlrewrite>