<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>


    <!-- 静态网页 -->
    <rule>
        <from>^/flow/(\w+)/(\w+)\.html$</from>
        <to>/sys/flow/page?m=$1&amp;f=$2</to>
    </rule>

</urlrewrite>